{
    "name": "mdr2025/pixel-app",
    "description": "PixelApp package",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "pixel company",
            "email": "email@example.com"
        }
    ],
    "require": {
        "ext-zip": "*",
        "ext-pdo": "*",
        "php": "^8.2.0",
        "laravel/framework": "^12.0",
        "laravel/passport": "^12.0",
        "muhammed-aldrubi/pixel-dom-pdf": "^2.0.0",
        "muhammed-aldrubi/exp-imp-management": "^2.0.0",
        "muhammed-aldrubi/temporary-files-handlers": "^2.0.0",
        "mdr2025/pixel-macroable-extenders" : "^2.0.0",
        "pixel-softwares/authorization-management": "^2.0.0",
        "pixel-softwares/crud-services": "^2.0.0",
        "pixel-softwares/custom-file-system": "^2.0.0",
        "pixel-softwares/custom-generators": "^2.0.0",
        "pixel-softwares/data-resource-instructors": "^2.0.0",
        "pixel-softwares/file-info-data-manager": "^2.0.0",
        "pixel-softwares/runtime-caching": "^2.0.0",
        "pixel-softwares/statistics": "^2.0.0",
        "pixel-softwares/validator-lib": "^2.0.0",
        "pixel-softwares/morphic-abstract-eloquent": "^2.0.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-query-builder": "^6.0",
        "spatie/laravel-translatable": "^6.0",
        "stancl/tenancy": "^3.9.0",
        "phpmailer/phpmailer": "^6.9",
        "sendgrid/sendgrid": "^8.0"
        
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.37",
        "phpunit/phpunit": "^11.0",
        "barryvdh/laravel-debugbar": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "PixelApp\\": "src/"
        },
        "exclude-from-classmap": [
            "src/ServiceProviders/LaravelProviderStubs",
            "src/Http/Middleware/PixelMiddlewareStubs",
            "src/Config/ConfigFiles",
            "src/Routes/RouteFileStubs",
            "src/Models/LaravelModelStubs",
            "src/Database/Migrations/MigrationStubs",
            "src/Database/Seeders/SeederStubs",
            "src/Console/ConsoleStubs",
            "src/Exceptions/ExceptionStubs"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "PixelApp\\ServiceProviders\\PixelAppPackageServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
   "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:MuhammedALDRUBI/temporary-files-handlers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:MuhammedALDRUBI/exp-imp-management.git"
        },
        {
